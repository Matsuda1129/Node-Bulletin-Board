<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <title>User list</title>
  </head>
  <body>
    <table class="table w-50 p-3 mx-auto">
      <tr>
        <th>名前</th>
        <th>email</th>
        <td>更新</td>
        <td>削除</td>
      </tr>
      <% user.forEach(function (value) { %>
      <tr>
        <td><%= value.name %></td>
        <td><%= value.email %></td>
        <td><a href="/users/edit/<%= value.id %>">更新</a></td>
        <td><a href="/users/delete/<%= value.id %>">削除</a></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
