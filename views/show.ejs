<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <title>User list</title>
  </head>
  <body>
    <table class="table w-50 p-3 mx-auto">
      <tr>
        <th>ID</th>
        <th>名前</th>
        <th>email</th>
        <th>更新</th>
        <th>削除</th>
      </tr>
      <% users.forEach(function (value) { %>
      <tr>
        <td><%= value.id %></td>
        <td><%= value.name %></td>
        <td><%= value.email %></td>
        <td><a href="/users/edit/<%= value[0]id %>">更新</a></td>
        <td><a href="/users/delete/<%= value[0]id %>">削除</a></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
